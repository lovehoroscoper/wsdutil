<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="lazyLoadingEnabled" value="false" />
	</settings>

	<typeAliases>
		<typeAlias alias="userInfoVo" type="org.gonetbar.ssa.entity.UserInfoVo" />
		<typeAlias alias="userProviderInfoVo" type="org.gonetbar.ssa.entity.UserProviderInfoVo" />
		<typeAlias alias="matcherInfo" type="org.gonetbar.ssa.entity.MatcherInfo" />
		<typeAlias alias="dbRoleAttribute" type="org.gonetbar.ssa.entity.DbRoleAttribute" />
		<typeAlias alias="aclGrantedAuthority" type="org.gonetbar.ssa.entity.AclGrantedAuthority" />
	</typeAliases>

	<mappers>
		<mapper resource="mapper/orm/mybatis/UserInfoVoMapper.xml" />
		<mapper resource="mapper/orm/mybatis/MatcherInfoMapper.xml" />
	</mappers>

</configuration>
