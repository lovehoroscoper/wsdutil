<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>


	<typeAliases>
		<!-- sso-acl -->
		<typeAlias alias="userInfoVo" type="org.godtips.sso.user.entity.UserInfoVo" />
		<typeAlias alias="userProviderInfoVo" type="org.godtips.sso.user.entity.UserProviderInfoVo" />
		<typeAlias alias="thirdProviderVo" type="org.godtips.sso.user.entity.ThirdProvider" />
		<typeAlias alias="aclUserVo" type="com.godtips.sso.acl.entity.AclUserVo" />
		<typeAlias alias="matcherInfo" type="com.godtips.sso.acl.entity.MatcherInfo" />
		<typeAlias alias="dbRoleAttribute" type="com.godtips.sso.acl.entity.DbRoleAttribute" />
		<typeAlias alias="aclGrantedAuthority" type="com.godtips.sso.acl.entity.AclGrantedAuthority" />
		<!-- end sso-acl -->
		
	</typeAliases>

	<mappers>
		<!-- sso-acl -->
		<mapper resource="mapper/com/wode/sso/acl/entity/MatcherInfoMapper.xml" />
		<mapper resource="mapper/com/wode/sso/user/entity/UserInfoVoMapper.xml" />
		<!-- end sso-acl -->
	</mappers>

</configuration>
